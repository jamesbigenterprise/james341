<!DOCTYPE html>
<html>
<head>
  <% include ../includes/header.ejs %>
  <link rel="stylesheet" type="text/css" href="/stylesheets/basicsAppMain.css" />
</head>

<body>
  <% include ../includes/nav.ejs %>
  <% include ../includes/navBasics.ejs %>
<div class="jumbotron text-center">
  <div class="container">
    <h1><%= title %></h1>

  </div>
</div>
<div class="container list">
    <form action="/basicsApp/admin/addProduct" method="POST">
      <label for="name">Item Name:</label>
      <input type="text" name="name">
      <br>
      <label for="price">Price:</label>
      <input type="text" name="price">
      <br>
      <label for="description">Description:</label>
      <input type="text" name="description">
      <br>
      <label for="imgUrl">Image URL:</label>
      <input type="text" name="imgUrl">
      <br>
      <label for="m">Male: </label>
      <input type="radio" name="gender" value="m"><br>
      <label for="f">Female: </label>
      <input type="radio" name="gender" value="f"><br>

      <button type="submit">Add</button>

    </form>
    </ul>
  </div>
</body>
</html>
